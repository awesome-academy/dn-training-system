<h1><%= t ".h1" %></h1>
<table class="table">
  <thead>
  <tr>
    <th scope="col"><%= t ".name" %></th>
    <th scope="col"><%= t ".description" %></th>
    <th scope="col"><%= t ".start_date" %></th>
    <th scope="col"><%= t ".due_date" %></th>
    <th scope="col"><%= t ".status" %></th>
  </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= link_to @course.name,  "" %></td>
      <td><%= @course.description %></td>
      <td><%= l(@course.start_date.to_date, format: :default) %></td>
      <td><%= l(@course.due_date.to_date, format: :default) %></td>
      <td><%= @course_user.status %></td>
    </tr>
  </tbody>
</table
