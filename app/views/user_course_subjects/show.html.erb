<h1><%= t :h1_show_user_course_subjects %></h1>
<table class="table">
  <thead>
  <tr>
    <th scope="col"><%= t :name_show_user_course_subjects %></th>
    <th scope="col"><%= t :status_show_user_course_subjects %></th>
    <th scope="col"><%= t :created_at_show_user_course_subjects %></th>
  </tr>
  </thead>
  <tbody>
    <% @user_course_subjects.each do |n| %>
      <tr>
        <td><%= link_to n.name_subject,  user_task_path(n.id) %></td>
        <td><%= n.status %></td>
        <td><%= l(n.created_at, format: :default) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
